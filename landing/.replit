# loctree Landing Page - Leptos WASM

run = "simple-http-server dist -p 8080 --index"

[nix]
channel = "stable-24_05"

[deployment]
run = ["sh", "-c", "simple-http-server dist -p 8080 --index"]
deploymentTarget = "static"
publicDir = "dist"

[[ports]]
localPort = 8080
externalPort = 80

[languages]
[languages.rust]
pattern = "**/*.rs"

[languages.rust.languageServer]
start = "rust-analyzer"

